grammar Ruby
  rule method_call
    primary_value call_op operation2 {
      def tree
        { :method_call => {
          :primary_value => primary_value.text_value,
          :operation2 => operation2.text_value }
        }
      end
    }
  end

  rule primary_value
    '1'
  end

  rule operation2
    'class'
  end

  rule call_op
    '.'
  end
end

